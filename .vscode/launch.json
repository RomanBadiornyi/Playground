{
    "version": "0.2.0",
    "configurations": [        
        {
            "name": "Run.FSharpPlayground",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build.Playground",
            "program": "${workspaceRoot}/FSharpPlayground/bin/Debug/net7.0/FSharpPlayground.dll",
            "args": [],
            "cwd": "${workspaceRoot}",
            "externalConsole": false,
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}